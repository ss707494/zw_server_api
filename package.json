  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "startdev": "npm-run-all --parallel watch:**",
    "startTsPro": "cross-env NODE_ENV=production node dist/server.js",
    "eTs": "tsc",
    "startTsDev": "cross-env node dist/server.js",
    "copyByShell": "ts-node file/copyStaticAssets.ts",
    "dealCommonModule": "ts-node my_modules/build.ts",
    "ddwatch:copyCommon": "nodemon -e json,ts --watch my_modules/src --watch my_modules/ts_self --exec npm run dealCommonModule",
    "watch:copy": "nodemon -e graphql --watch src --exec npm run copyByShell",
    "watch:src": "nodemon -e js,ts --watch src --exec npm run eTs",
    "watch:dist": "nodemon -e js,ts,graphql --watch dist --watch package.json --delay 150ms --exec npm run startTsDev",
    "tsBuild": "npm run eTs && npm run copyByShell",
    "start": "pm2 start ./file/start.js --name dw",
    "test": "echo \"Error: no test specified\" && exit 1",
    "initBuild": "babel-node file/initBuild.js",
    "typeorm_schema:log": "typeorm schema:log",
    "migration:generate": "typeorm migration:generate -n UserMigration",
    "typeorm_migration:run": "typeorm migration:run"
  },
