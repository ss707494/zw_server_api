{
  "name": "graphql",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "startdev": "npm-run-all --parallel watch:**",
    "startTsPro": "cross-env NODE_ENV=production node dist/server.js",
    "eTs": "tsc",
    "startTsDev": "cross-env node dist/server.js",
    "copyByShell": "ts-node file/copyStaticAssets.ts",
    "watch:copy": "nodemon -e graphql --watch src/schema --exec npm run copyByShell",
    "watch:src": "nodemon -e js,ts --watch src --exec npm run eTs",
    "watch:dist": "nodemon -e js,ts,graphql --watch dist --delay 150ms --exec npm run startTsDev",
    "tsBuild": "npm run eTs && npm run copyByShell",
    "start": "npm run tsBuild && npm run startTsPro",
    "copySchema": "babel -d dist/schema src/schema --copy-files",
    "copyAssets": "babel -d dist/assets src/assets --copy-files",
    "copyFile": "npm run copyAssets & npm run copySchema",
    "babel": "npm run copyFile && babel -d dist/ src/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "initBuild": "babel-node file/initBuild.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-es2017": "^7.0.0-beta.53",
    "@babel/preset-typescript": "^7.7.4",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.3.1",
    "@types/shelljs": "^0.8.6",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-import-graphql": "^2.6.2",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-regenerator": "^6.20.0",
    "cross-env": "^5.2.0",
    "module-alias": "^2.2.2",
    "npm-run-all": "^4.1.5",
    "shelljs": "^0.8.3",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@types/graphql": "^14.5.0",
    "apollo-server-express": "^2.4.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "date-format": "^2.1.0",
    "dotenv": "^8.1.0",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "express-jwt": "^5.3.1",
    "graphql-import": "^0.7.1",
    "graphql-tools": "^4.0.4",
    "graphql-type-json": "^0.3.1",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "merge-graphql-schemas": "^1.5.8",
    "multer": "^1.4.2",
    "mysql": "mysqljs/mysql",
    "nodemon": "^1.18.10",
    "uuid": "^3.3.2"
  },
  "_moduleAliases": {
    "@": "./dist"
  }
}
